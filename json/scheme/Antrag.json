{
  "$schema" : "https://json-schema.org/draft/2019-09/schema",
  "type" : "object",
  "properties" : {
    "AusgabenBerechnungen" : {
      "type" : "object",
      "properties" : {
        "id" : {
          "type" : "integer"
        }
      }
    },
    "email" : {
      "type" : "string",
      "minLength" : 6,
      "maxLength" : 127,
      "pattern" : "^(.+)@(.+)$"
    },
    "foerdergegenstand" : {
      "type" : "string"
    },
    "foerderprogramm" : {
      "type" : "string"
    },
    "id" : {
      "type" : "integer"
    },
    "username" : {
      "type" : [ "string", "null" ]
    }
  },
  "required" : [ "foerdergegenstand", "foerderprogramm", "id" ]
}